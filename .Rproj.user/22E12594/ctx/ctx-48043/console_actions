        "924\n925\n926\n927\n928\n929\n930\n931\n932\n933\n934\n935\n936\n937\n938\n939\n940\n941\n942\n943\n944\n945\n946\n947\n948\n949\n950\n951\n952\n953\n954\n955\n956\n957\n958\n959\n960\n961\n962\n963\n964\n965\n966\n967\n968\n969\n970\n971\n972\n973\n974\n975\n976\n977\n978\n979\n980\n981\n982\n983\n984\n985\n986\n987\n988\n989\n990\n991\n992\n993\n994\n995\n996\n997\n998\n999\n1000\n",
        "> ",
        "",
        "> ",
        "hist(rec)",
        "> ",
        "colMeans(rec < .05)",
        "[1] 0.079 0.155\n",
        "> ",
        "colMeans(rec < .1)",
        "[1] 0.126 0.270\n",
        "> ",
        "fullTest <- function(Y, ord, K = 4, bs = 200){",
        "+ ",
        "  n <- dim(Y)[1]",
        "+ ",
        "  p <- dim(Y)[2]",
        "+ ",
        "  ",
        "+ ",
        "  ",
        "+ ",
        "  .fullTestHelper <- function(i){",
        "+ ",
        "    out <- singleTestcpp(X = Y[, ord[1:(i-1)], drop = F], Y = Y[, ord[i], drop = F], K = K, bs = bs)",
        "+ ",
        "    out$nullDist <- out$nullDist * sqrt(n / (n - i + 1))",
        "+ ",
        "    return(out)",
        "+ ",
        "  }",
        "+ ",
        "  ",
        "+ ",
        "  out <- lapply(2:p, .fullTestHelper)",
        "+ ",
        "  nullDist <- do.call(\"cbind\", lapply(out, function(x){x$nullDist}))",
        "+ ",
        "  ",
        "+ ",
        "  testStats <- do.call(\"cbind\", lapply(out, function(x){x$test}))",
        "+ ",
        "  ",
        "+ ",
        "  pVal <- mean(apply(nullDist, MAR = 1, max) > max(testStats))",
        "+ ",
        "  ",
        "+ ",
        "  return(pVal)",
        "+ ",
        "}",
        "> ",
        "",
        "> ",
        "",
        "> ",
        "fullTestNonAdj <- function(Y, ord, K = 4, bs = 200){",
        "+ ",
        "  n <- dim(Y)[1]",
        "+ ",
        "  p <- dim(Y)[2]",
        "+ ",
        "  ",
        "+ ",
        "  ",
        "+ ",
        "  .fullTestHelper <- function(i){",
        "+ ",
        "    out <- singleTestcpp(X = Y[, ord[1:(i-1)], drop = F], Y = Y[, ord[i], drop = F], K = K, bs = bs)",
        "+ ",
        "    # out$nullDist <- out$nullDist * sqrt(n / (n - i + 1))",
        "+ ",
        "    return(out)",
        "+ ",
        "  }",
        "+ ",
        "  ",
        "+ ",
        "  out <- lapply(2:p, .fullTestHelper)",
        "+ ",
        "  nullDist <- do.call(\"cbind\", lapply(out, function(x){x$nullDist}))",
        "+ ",
        "  ",
        "+ ",
        "  testStats <- do.call(\"cbind\", lapply(out, function(x){x$test}))",
        "+ ",
        "  ",
        "+ ",
        "  pVal <- mean(apply(nullDist, MAR = 1, max) > max(testStats))",
        "+ ",
        "  ",
        "+ ",
        "  return(pVal)",
        "+ ",
        "}",
        "> ",
        "",
        "> ",
        "",
        "> ",
        "library(cdcs)",
        "> ",
        "p <- 75",
        "> ",
        "n <- 150",
        "> ",
        "",
        "> ",
        "rec <- matrix(0, 1000, 2)",
        "> ",
        "for(i in 1:1000){",
        "+ ",
        "  cat(i)",
        "+ ",
        "  cat(\"\\n\")",
        "+ ",
        "  dat <- rDAG(p, n, maxInDegree = 4, lowEdge = .1, highEdge = .5, dist = \"lognorm\")",
        "+ ",
        "  rec[i, 1] <- fullTest(dat$Y, 1:p, K = 4, bs = 200)",
        "+ ",
        "  rec[i, 2] <- fullTestNonAdj(dat$Y, 1:p, K = 4, bs = 200)",
        "+ ",
        "}",
