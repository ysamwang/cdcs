sample.size <- 300
rep.runs <- 15
n.list <- c(2000)
d.list <- c("gauss", "unif", "laplace", "gamma")
func.list <- c("poly", "fourier", "GP")
basis.list <- c("poly", "bspline")
param.grid <- expand.grid(rep(n.list, sample.size / rep.runs), d.list, func.list, basis.list)
param.grid
